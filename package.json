{
  "name": "pickassistant_saas",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "dev": "run-p dev:*",
    "dev:plasmo": "plasmo dev --target=firefox-mv2",
    "dev:next": "next dev --port 1947",
    "build": "run-p build:*",
    "build:plasmo": "plasmo build",
    "build:next": "next build",
    "lint": "next lint",
    "prettier": "prettier --check .",
    "fix": "prettier --write . && next lint --fix"
  },
  "dependencies": {
    "appwrite": "^11.0.0",
    "jotai": "^2.1.1",
    "next": "^13.4.5",
    "node-appwrite": "^9.0.0",
    "plasmo": "^0.75.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "server-only": "^0.0.1",
    "stripe": "^12.9.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@plasmohq/rps": "^1.8.6",
    "@types/node": "^20.3.0",
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.14",
    "encoding": "^0.1.13",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.4.5",
    "eslint-config-prettier": "^8.8.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "^5.1.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "storage",
      "cookies"
    ]
  }
}
