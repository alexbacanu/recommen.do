{
  "name": "pickassistant_saas",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "dev": "run-p dev:*",
    "dev:plasmo": "plasmo dev --target=firefox-mv2",
    "dev:next": "next dev --port 1947",
    "build": "run-p build:*",
    "build:plasmo": "plasmo build",
    "build:next": "next build",
    "lint": "next lint",
    "prettier": "prettier --check .",
    "fix": "prettier --write . && next lint --fix"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.1",
    "@plasmohq/storage": "^1.6.1",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "appwrite": "^11.0.0",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "framer-motion": "^10.12.16",
    "jotai": "^2.1.1",
    "lucide-react": "^0.242.0",
    "next": "^13.4.5",
    "node-appwrite": "^9.0.0",
    "plasmo": "^0.75.2",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "server-only": "^0.0.1",
    "stripe": "^12.9.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@plasmohq/rps": "^1.8.6",
    "@types/node": "^20.3.0",
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.14",
    "encoding": "^0.1.13",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.4.5",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwind-merge": "^1.13.1",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "^5.1.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "storage",
      "cookies"
    ],
    "key": "$CRX_PUBLIC_KEY",
    "browser_specific_settings": {
      "gecko": {
        "id": "$FIREFOX_EXT_ID",
        "strict_min_version": "70.0"
      }
    }
  }
}
